<template name="3a755df8">
  <view class="_view data-v-43b8d8f2">
    <view class="_view data-v-43b8d8f2 header {{[isH5Plus ? 'status' : '']}}">
      <view class="_view data-v-43b8d8f2 userinfo">
        <view class="_view data-v-43b8d8f2 face">
          <image src="{{userinfo.face}}" class="_image data-v-43b8d8f2"></image>
        </view>
        <view class="_view data-v-43b8d8f2 info">
          <view class="_view data-v-43b8d8f2 username">{{userinfo.username}}</view>
          <view class="_view data-v-43b8d8f2 integral">积分:{{userinfo.integral}}</view>
        </view>
      </view>
      <view class="_view data-v-43b8d8f2 setting">
        <image src="../../static/HM-PersonalCenter/setting.png" class="_image data-v-43b8d8f2"></image>
      </view>
    </view>
    <view class="_view data-v-43b8d8f2 orders">
      <view class="_view data-v-43b8d8f2 box">
        <view class="_view data-v-43b8d8f2 label" wx:key="row.name" hover-class="hover" hover-stay-time="50" bindtap="handleProxy" data-eventid="{{'2Gx-0-'+index}}" data-comkey="{{$k}}" wx:for="{{orderTypeLise}}" wx:for-index="index" wx:for-item="row">
          <view class="_view data-v-43b8d8f2 icon">
            <view class="_view data-v-43b8d8f2 badge" wx:if="{{row.badge>0}}">{{row.badge}}</view>
            <image src="{{'../../static/HM-PersonalCenter/'+row.icon}}" class="_image data-v-43b8d8f2"></image>
          </view> {{row.name}} </view>
      </view>
    </view>
    <view class="_view data-v-43b8d8f2 list" wx:key="list" wx:for="{{severList}}" wx:for-index="list_i" wx:for-item="list">
      <view class="_view data-v-43b8d8f2 li {{[li_i == list.length - 1 ? 'noborder' : '']}}" bindtap="handleProxy" hover-class="hover" hover-stay-time="50" wx:key="li.name" data-eventid="{{'a90-1-'+list_i+'-'+li_i}}" data-comkey="{{$k}}" wx:for="{{list}}"
        wx:for-index="li_i" wx:for-item="li">
        <view class="_view data-v-43b8d8f2 icon">
          <image src="{{'../../static/HM-PersonalCenter/sever/'+li.icon}}" class="_image data-v-43b8d8f2"></image>
        </view>
        <view class="_view data-v-43b8d8f2 text">{{li.name}}</view>
        <image class="_image data-v-43b8d8f2 to" src="../../static/HM-PersonalCenter/to.png"></image>
      </view>
    </view>
  </view>
</template>